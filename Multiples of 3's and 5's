import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        System.out.print("Enter number:");
        Scanner sc = new Scanner(System.in);
        int num =sc.nextInt();
        int sum = 0;
        for (int i = 0; i < num; i++) {
            if (i % 3 == 0 || i % 5 == 0){
                sum = sum + i;
            }
        }
        System.out.println("The sum of 3's and 5's is: "+sum);
    }
}
